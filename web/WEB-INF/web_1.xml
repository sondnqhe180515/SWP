<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>AccControll</servlet-name>
        <servlet-class>Controller.AccControll</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userDetail</servlet-name>
        <servlet-class>Controller.userDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addUserServlet</servlet-name>
        <servlet-class>Controller.addUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteUserServlet</servlet-name>
        <servlet-class>Controller.deleteUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>prescriptionServlet</servlet-name>
        <servlet-class>Controller.prescriptionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>prescriptionController</servlet-name>
        <servlet-class>Controller.prescriptionController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateMedicalRecordServlet</servlet-name>
        <servlet-class>Controller.CreateMedicalRecordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreatePrescriptionServlet</servlet-name>
        <servlet-class>Controller.CreatePrescriptionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AccControll</servlet-name>
        <url-pattern>/AccControll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userDetail</servlet-name>
        <url-pattern>/userDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addUserServlet</servlet-name>
        <url-pattern>/addUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteUserServlet</servlet-name>
        <url-pattern>/deleteUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>prescriptionServlet</servlet-name>
        <url-pattern>/prescriptionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>prescriptionController</servlet-name>
        <url-pattern>/prescriptionController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateMedicalRecordServlet</servlet-name>
        <url-pattern>/CreateMedicalRecordServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreatePrescriptionServlet</servlet-name>
        <url-pattern>/CreatePrescriptionServlet</url-pattern>
    </servlet-mapping>
</web-app>
